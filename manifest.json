{
  "manifest_version": 2,
  "name": "Super Search",
  "description": "Know what you search and help you get to what you want to know faster",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js",
      "backend_api_caller.js"
    ]
  },
  "permissions": [
	"storage",
    "webNavigation",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"

  ],
  "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*",
    "*.svg",
    "*.SVG",
    "cytoscape.js",
    "*/iframe-graph.html"
  ]

}
