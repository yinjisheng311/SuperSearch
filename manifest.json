{
  "manifest_version": 2,
  "name": "Super Search",
  "description": "Know what you search and help you get to what you want to know faster",
  "version": "1.0",
  "content_scripts": [{
    "css": ["/components/iframe-graph/iframe-graph.css", "styles.css"],
    "js": ["insert-iframe-graph.js","jquery-3.3.1.min.js", "jquery.gsap.min.js", "TweenLite.min.js", "TweenMax.min.js", "TimelineLite.min.js", "/components/iframe-graph/iframe-graph.js", "materialdesign.js"],
    "matches": ["https://google.com.sg/*", "https://www.google.com/*"]
  }],
  "permissions": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self'",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*",
    "*.svg",
    "*.SVG",
    "*/iframe-graph.html"
  ],
  "background": {
    "scripts": ["materialdesign.js", "jquery-3.3.1.min.js", "jquery.gsap.min.js", "TweenLite.min.js", "TweenMax.min.js", "TimelineLite.min.js"],
    "persistent": true
  }
}
